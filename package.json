{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "reactstrap": "^8.0.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"node ./mock-api/http.js\" \"react-scripts start\"",
    "start-front": "react-scripts start",
    "start-api": "node ./mock-api/http.js",
    "test": "concurrently --kill-others \"node ./mock-api/http.js\" \"react-scripts start\" \"jest ./test/* --watch\" \"react-scripts test --watch\"",
    "test-user": "concurrently --kill-others \"node ./mock-api/http.js\" \"react-scripts start\" \"jest ./test/* --watch\"",
    "test-components": "react-scripts test",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "httpdispatcher": "^2.1.2",
    "jest-puppeteer": "^4.1.1",
    "puppeteer": "^1.14.0",
    "react-test-renderer": "^16.8.6"
  }
}
